{
  "name": "building",
  "version": "0.0.1",
  "description": "Building manager",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/williamthome/building.git"
  },
  "private": true,
  "scripts": {
    "build": "rimraf dist && npx tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "build:start": "npm run build && npm start",
    "dev": "npm run build && ts-node-dev --respawn --transpile-only src/index.ts",
    "debug": "npm run build && npm run start -- --inspect=0.0.0.0:9222 --nolazy",
    "test": "npx jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
    "test:verbose": "npx jest --passWithNoTests --runInBand --detectOpenHandles",
    "test:unit": "npm test -- --watch \"^(.*/)*(.*\\.)*(spec)\\.ts$\"",
    "test:unit:verbose": "npm run test:verbose -- --watch \"^(.*/)*(.*\\.)*(spec)\\.ts$\"",
    "test:integration": "npm test -- --watch \"^(.*/)*(.*\\.)*(test)\\.ts$\"",
    "test:integration:verbose": "npm run test:verbose -- --watch \"^(.*/)*(.*\\.)*(test)\\.ts$\"",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:clear": "npx jest --clearCache"
  },
  "keywords": [
    "building",
    "manager",
    "civil",
    "engineering"
  ],
  "author": "William Fank Thom√©",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.32",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.3",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "fastify": "^3.7.0",
    "jsonwebtoken": "^8.5.1",
    "lilinjector": "0.0.5",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.2"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
